{
  "name": "codephoenixdev/moneypal-laravel-gateway",
  "description": "A Laravel payment gateway package for MoneyPal integration",
  "type": "library",
  "keywords": ["laravel", "payment", "gateway", "money", "moneypal"],
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Kamyab",
      "email": "info@kamyabhost.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "CodePhoenixDev\\MoneyPal\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "post-autoload-dump": [
      "php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
